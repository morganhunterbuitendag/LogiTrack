{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "*.html",
          "*.css",
          "*.js",
          "src/lib/ors.js",
          "data/**",
          "processors.json",
          "producers.json",
          "distances.json",
          "pending-users.json",
          "reset-tokens.json",
          "users.json",
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
